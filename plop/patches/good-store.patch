+import { Event } from 'types/Event';
+import { Tastic } from 'types/tastic';
+
+const EventTastic: React.FC<Tastic<{ items: Event }, { events: any }>> = ({ data }) => {
+  const events = data?.events?.dataSource?.data?.items;
+
+  if (!events || events.length == 0) {
+    return <div>No events near you -- check back again soon!</div>;
+  }
+
+  return (
+    <>
+      {events.map((event: Event) => (
+        <div key={event.id}>
+          <div className="columns-3">
+            <div>{event.date}</div>
+            <div>{event.venue}</div>
+            <div>{event.artist}</div>
+          </div>
+        </div>
+      ))}
+    </>
+  );
+};
+
+export default EventTastic;
