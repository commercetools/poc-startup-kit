+import { Product } from '@commercetools/frontend-domain-types/product/Product';
+import { Facet } from '@commercetools/frontend-domain-types/result/Facet';
+import { useFormat } from 'helpers/hooks/useFormat';
+import { Category } from 'types/category';
+import { RangeFacet as RangeFacetType } from 'types/facet';
+import { TermsFacet } from '../../types';
+import ColorFacet from '../facets/color';
+import RangeFacet from '../facets/range';
+import SortFacet from '../facets/sort';
+import TermFacet from '../facets/term';
+
+interface Props {
+  categories: Category[];
+  products: Product[];
+  previousCursor: string;
+  nextCursor: string;
+  category: string;
+  facets: Facet[];
+  totalProducts: number;
+}
+
+const DesktopFacets: React.FC<Props> = ({
+  products,
+  totalProducts,
+  previousCursor,
+  nextCursor,
+  category,
+  categories,
+  facets,
+}) => {
+  const { formatMessage: formatProductMessage } = useFormat({ name: 'product' });
+
+  const color = facets?.find((item) => item.identifier === 'variants.attributes.color') as TermsFacet;
+  const commonSize = facets?.find((item) => item.identifier === 'variants.attributes.commonSize') as TermsFacet;
+  const price = facets?.find((item) => item.identifier === 'variants.price') as RangeFacetType;
+
+  return (
+    <div className="hidden items-center justify-between border-b border-neutral-400 pb-16 pt-48 lg:flex">
+      <div className="flex items-center gap-12">
+        <div>{!!color && color.terms?.length > 1 && <ColorFacet color={color} />}</div>
+        <div>{!!commonSize && commonSize.terms?.length > 1 && <TermFacet facet={commonSize} />}</div>
+        <div>{!!price && <RangeFacet range={price} />}</div>
+      </div>
+
+      <div className="flex items-center gap-18">
+        <span className="text-14 text-secondary-black">
+          {totalProducts ?? 0} {formatProductMessage({ id: 'items', defaultMessage: 'Items' })}
+        </span>
+        <div>
+          <SortFacet />
+        </div>
+      </div>
+    </div>
+  );
+};
+
+export default DesktopFacets;
