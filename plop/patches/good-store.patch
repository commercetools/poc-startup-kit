+import React, { useState } from 'react';
+import { Variant as VariantType } from '@commercetools/frontend-domain-types/product/Variant';
+import Variant from 'components/commercetools-ui/organisms/variant';
+import { UIProduct } from './types';
+import { getAttributeValue } from './variant-selector';
+
+type Props = {
+  product: UIProduct;
+  variantSelectors: string[];
+  variant: VariantType;
+  isVariantGroup?: boolean;
+  onChangeVariant: (sku: string) => void;
+};
+
+interface UIVariant {
+  variant: VariantType;
+  quantity: number;
+}
+
+const groupedVariants = (items: VariantType[], variantSelectors: string[]): VariantType[][] => {
+  const variants = [...items];
+  const grouped: VariantType[][] = [];
+  while (variants.length) {
+    const [current] = variants.splice(0, 1);
+    const currentAttributeValue = getAttributeValue(variantSelectors[0], current.attributes);
+    const index = grouped.findIndex(
+      (group) =>
+        getAttributeValue(variantSelectors[0], group?.[0].attributes) === currentAttributeValue &&
+        typeof currentAttributeValue !== 'undefined',
+    );
+    if (index > -1) {
+      grouped[index].push(current);
+    } else if (typeof currentAttributeValue !== 'undefined') {
+      grouped.push([current]);
+    }
+  }
+  return grouped.map((group) =>
+    group.sort(
+      (a, b) =>
+        getAttributeValue(variantSelectors[1], a.attributes) - getAttributeValue(variantSelectors[1], b.attributes),
+    ),
+  );
+};
+
+const GridVariantSelector: React.FC<Props & React.HTMLAttributes<HTMLDivElement>> = ({
+  product,
+  variantSelectors,
+  variant,
+  isVariantGroup,
+  onChangeVariant,
+  className,
+}) => {
+  const [grouped] = useState<VariantType[][]>(groupedVariants(product.variants, variantSelectors));
+  const [selectedFirstVariantIdx, setSelectedFirstVariantIdx] = useState(0);
+  const [firstSelectedVariant, setFirstSelectedVariant] = useState(grouped?.[0]?.[grouped?.[0]?.length - 1]);
+
+  const colSpan = grouped?.[0].length + 2;
+
+  const handleChangeFirstSelector = (sku: string) => {
+    onChangeVariant(sku);
+    const variant = product.variants.find((variant) => variant.sku === sku);
+    if (variant) {
+      const currentAttributeValue = getAttributeValue(variantSelectors[0], variant.attributes);
+
+      const i = grouped.findIndex(
+        (group) =>
+          getAttributeValue(variantSelectors[0], group?.[0].attributes) === currentAttributeValue &&
+          typeof currentAttributeValue !== 'undefined',
+      );
+      if (i > -1) {
+        setSelectedFirstVariantIdx(i);
+        // select last Item in each group as the selected variant to comply with Map in Other variant selectors
+        setFirstSelectedVariant(variant);
+      }
+    }
+  };
+
+  const handleChangeSecondSelector = (sku: string) => {
+    onChangeVariant(sku);
+  };
+
+  return (
+    <div className="w-3/4">
+      <Variant
+        className="mt-25 border-b border-b-neutral-400 pb-20"
+        variants={product?.variants}
+        currentVariant={firstSelectedVariant}
+        attribute={variantSelectors[0]}
+        isVariantGroup={isVariantGroup}
+        onClick={handleChangeFirstSelector}
+      />
+      <Variant
+        className="mt-25 border-b border-b-neutral-400 pb-20"
+        variants={grouped[selectedFirstVariantIdx]}
+        currentVariant={variant}
+        attribute={variantSelectors[1]}
+        onClick={handleChangeSecondSelector}
+      />
+    </div>
+  );
+};
+
+export default GridVariantSelector;
