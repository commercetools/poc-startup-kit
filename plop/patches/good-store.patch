+import { BaseApi } from './BaseApi';
+interface Event {
+  id: string;
+  key: string;
+  date?: Date;
+  artist?: string;
+  venue?: string;
+}
+
+interface Result {
+  total?: number;
+  count?: number;
+  items: Event[];
+}
+
+export class EventApi extends BaseApi {
+  query: () => Promise<Result> = async () => {
+    try {
+      const result = await this.getApiForProject()
+        .customObjects()
+        .withContainer({ container: 'event' })
+        .get()
+        .execute();
+      const items: Array<Event> = result.body.results.map((customObject) => {
+        const event: Event = {
+          ...customObject.value,
+          id: customObject.id,
+          key: customObject.key,
+        };
+        return event;
+      });
+      const res: Result = {
+        items: items,
+      };
+      return res;
+    } catch (error) {
+      //TODO: better error, get status code etc...
+      throw new Error(`get events failed. ${error}`);
+    }
+  };
+}
