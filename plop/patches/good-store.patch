+import { LineItem } from '@commercetools/frontend-domain-types/cart/LineItem';
+import { Variant } from '@commercetools/frontend-domain-types/product/Variant';
+
+export const bundleSlugToAttributes: Record<string, Record<string, string>> = {
+  Hemming: { 'dxl-cuff': 'Cuff', 'dxl-inseam': 'Inseam' },
+  Subscription: { subscription_interval_days: 'Every' },
+};
+export const bundleSlugToEmptyLabel: Record<string, string> = {
+  Hemming: 'Unfinished',
+  Subscription: 'None',
+};
+
+const getLable = (value: Record<string, string> | string | boolean) => {
+  if (value === true) {
+    return 'Y';
+  }
+  if (value === false) {
+    return 'N';
+  }
+  if (typeof value === 'object') {
+    return value.label;
+  }
+  return value;
+};
+
+export const getBundledPrice = (lineItem: LineItem): number => {
+  return (lineItem.variant?.attributes?.['bundles'] as LineItem[]).reduce(
+    (prev, curr) => prev + (curr.price?.centAmount || 0),
+    0,
+  );
+};
+
+export const getSelectedBundleLabel = (variant?: Variant, name?: string) => {
+  const bundleItemAttributes = bundleSlugToAttributes[name || ''];
+  if (bundleItemAttributes) {
+    return Object.keys(bundleItemAttributes)
+      .map((attribute) => `${bundleItemAttributes[attribute]} ${getLable(variant?.attributes?.[attribute])}`)
+      .join(', ');
+  }
+  return '';
+};
+
+export const getEmptyLabel = (name?: string) => {
+  return bundleSlugToEmptyLabel[name || ''];
+};
+
+export const bundleItems = (lineItems: (LineItem & { custom: any })[]): (LineItem & { custom: any })[] => {
+  const bundles = lineItems?.filter((item) => !!item.custom?.fields?.parentId);
+  const items = lineItems?.filter((item) => !item.custom?.fields?.parentId);
+
+  return items?.map((item) => {
+    const itemBundles = bundles?.filter((bundle) => bundle.custom.fields?.parentId === item.lineItemId);
+    // @ts-ignore
+    item.variant?.attributes['bundles'] = [];
+    itemBundles?.forEach((bundle) => {
+      // @ts-ignore
+      item.variant?.attributes['bundles'].push(bundle);
+    });
+    return item;
+  });
+};
