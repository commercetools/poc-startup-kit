+/* eslint-disable @typescript-eslint/ban-ts-comment */
+import React, { HTMLAttributes, useState } from 'react';
+import Typography from 'components/commercetools-ui/atoms/typography';
+
+type StarProps = {
+  changeGradeIndex: (i: number) => void;
+  grade: string;
+  index: number;
+  style: React.CSSProperties;
+};
+
+const Star: React.FC<StarProps> = ({ changeGradeIndex, grade, index, style }) => {
+  const changeGrade = (e: React.MouseEvent<HTMLInputElement>) => {
+    // @ts-ignore
+    changeGradeIndex(+e.target.value);
+  };
+
+  return (
+    <label className="relative cursor-pointer">
+      <input
+        type="radio"
+        name="rating"
+        id={grade}
+        value={index}
+        className="absolute top-0 left-0 hidden"
+        onClick={changeGrade}
+      />
+      <svg
+        viewBox="0 0 24 24"
+        fill="none"
+        stroke="#393939"
+        strokeWidth="1"
+        strokeLinecap="round"
+        strokeLinejoin="round"
+        style={style}
+      >
+        <polygon points="12 2 15.09 8.26 22 9.27 17 14.14 18.18 21.02 12 17.77 5.82 21.02 7 14.14 2 9.27 8.91 8.26 12 2"></polygon>
+      </svg>
+    </label>
+  );
+};
+
+type RatingStarsProps = {
+  onUpdate?: (i: number) => void;
+  averageRating?: number;
+  readonly?: boolean;
+  style?: React.CSSProperties;
+};
+
+const RatingStars: React.FC<RatingStarsProps & HTMLAttributes<HTMLDivElement>> = ({
+  onUpdate,
+  averageRating,
+  readonly,
+  style,
+  className,
+}) => {
+  const [gradeIndex, setGradeIndex] = useState<number | undefined>(averageRating);
+  const GRADES: string[] = ['Poor', 'Fair', 'Good', 'Very good', 'Excellent'];
+  const defaultStyle = {
+    width: '40px',
+    height: '40px',
+    ...style,
+  };
+
+  const changeGradeIndex = (index: number) => {
+    if (!readonly) {
+      setGradeIndex(index);
+      if (onUpdate) onUpdate(index);
+    }
+  };
+
+  return (
+    <div className={className}>
+      {!readonly && (
+        <Typography className="mb-16">
+          {typeof gradeIndex !== 'undefined' ? GRADES[gradeIndex] : "You didn't review yet"}
+        </Typography>
+      )}
+      <div className="flex flex-row items-center">
+        {GRADES.map((grade, index) => (
+          <Star
+            index={index}
+            key={grade}
+            grade={grade}
+            changeGradeIndex={changeGradeIndex}
+            style={
+              typeof gradeIndex !== 'undefined' && gradeIndex >= index
+                ? { fill: 'gold', ...defaultStyle }
+                : defaultStyle
+            }
+          />
+        ))}
+      </div>
+    </div>
+  );
+};
+
+export default RatingStars;
