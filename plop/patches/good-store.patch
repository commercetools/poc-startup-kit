@@ -0,0 +1,98 @@
+import { BaseApi } from './BaseApi';
+import { calculateNextDeliveryDate, getSubscriptionGroups } from '../utils/Cart';
+import { ProductApi } from '../apis/ProductApi';
+import { CartApi } from '../apis/CartApi';
+import { SubscriptionMapper } from '../mappers/SubscriptionMapper';
+import { ProductQuery } from '@commercetools/frontend-domain-types/query/ProductQuery';
+import { Product } from '@commercetools/frontend-domain-types/product/Product';
+import { Cart } from '@commercetools/frontend-domain-types/cart/Cart';
+import { Order } from '@commercetools/frontend-domain-types/cart/Order';
+
+export class SubscriptionApi extends BaseApi {
+  getSubscriptionsForAccount = async (accountId: string, config: Record<string, string>): Promise<Cart[]> => {
+    try {
+      const locale = await this.getCommercetoolsLocal();
+      if (config.orderCustomFieldNameOnCart && accountId) {
+        const response = await this.getApiForProject()
+          .carts()
+          .get({
+            queryArgs: {
+              where: [`customerId="${accountId}"`, `custom(fields(${config.orderCustomFieldNameOnCart} is defined))`],
+              expand: [
+                'lineItems[*].variant',
+                'store',
+                `custom.fields.${config.orderCustomFieldNameOnCart}`,
+                `custom.fields.${config.productCustomFieldNameOnCart}`,
+              ],
+            },
+          })
+          .execute();
+
+        return response.body.results.map((commercetoolsCart) =>
+          SubscriptionMapper.commercetoolsCartToCart(commercetoolsCart, locale, config),
+        );
+      }
+      console.error('config for subscriptions is not in place');
+      return [];
+    } catch (error) {
+      throw new Error(`Get subscriptions for account failed: ${error}`);
+    }
+  };
+
+  handleSubscriptionsOnOrder = async (
+    productApi: ProductApi,
+    cartApi: CartApi,
+    cart: Cart,
+    order: Order,
+    subscriptionConfig: Record<string, string>,
+    bundlesConfig: Record<string, string>,
+  ): Promise<void> => {
+    if (
+      subscriptionConfig.subscriptionProductAttributeName &&
+      subscriptionConfig.customTypeKeyOnCart &&
+      subscriptionConfig.orderCustomFieldNameOnCart &&
+      subscriptionConfig.productCustomFieldNameOnCart &&
+      subscriptionConfig.skuCustomFieldNameOnCart &&
+      subscriptionConfig.isActiveCustomFieldNameOnCart &&
+      subscriptionConfig.isSubscriptionCustomFieldNameOnCart &&
+      subscriptionConfig.nextAccuranceCustomFieldNameOnCart
+    ) {
+      const subscriptionGroups = getSubscriptionGroups(cart, bundlesConfig);
+
+      if (subscriptionGroups) {
+        for await (const sku of Object.keys(subscriptionGroups)) {
+          const interval = subscriptionConfig?.subscriptionProductAttributeName
+            ? parseInt(
+                subscriptionGroups[sku].variant?.attributes?.[subscriptionConfig.subscriptionProductAttributeName]?.key,
+              )
+            : 0;
+          const nextDeliveryDate = calculateNextDeliveryDate(subscriptionGroups[sku].variant, interval);
+          const productQuery: ProductQuery = {
+            skus: [sku],
+          };
+          const subscriptionProduct: Product = await productApi.getProduct(productQuery);
+
+          //create cart
+          let nextCart: Cart = await cartApi.replicateCart(order.cartId);
+          nextCart = await cartApi.setCartExpirationDays(nextCart, interval + 1);
+          nextCart = await cartApi.removeAllLineItems(nextCart);
+          nextCart = await cartApi.addItemsToCart(nextCart, subscriptionGroups[sku].lineItems);
+          nextCart = await cartApi.setCustomType(nextCart, subscriptionConfig.customTypeKeyOnCart, {
+            [subscriptionConfig.orderCustomFieldNameOnCart]: {
+              typeId: 'order',
+              id: order.cartId,
+            },
+            [subscriptionConfig.productCustomFieldNameOnCart]: {
+              typeId: 'product',
+              id: subscriptionProduct.productId,
+            },
+            [subscriptionConfig.skuCustomFieldNameOnCart]: sku,
+            [subscriptionConfig.nextAccuranceCustomFieldNameOnCart]: nextDeliveryDate,
+            [subscriptionConfig.isSubscriptionCustomFieldNameOnCart]: true,
+            [subscriptionConfig.isActiveCustomFieldNameOnCart]: true,
+          });
+        }
+      }
+    }
+  };
+}
