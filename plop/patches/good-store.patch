+import { BaseApi } from './BaseApi';
+import { Review } from '../interfaces/review';
+
+export class ReviewApi extends BaseApi {
+  getReviewsForProduct = async (productId: string): Promise<Review[]> => {
+    try {
+      const locale = await this.getCommercetoolsLocal();
+      const response = await this.getApiForProject()
+        .reviews()
+        .get({
+          queryArgs: {
+            where: [`target(id="${productId}")`],
+            expand: ['customer'],
+          },
+        })
+        .execute();
+
+      return response.body.results.map((commercetoolsReview) => ({
+        id: commercetoolsReview.id,
+        rating: commercetoolsReview.rating,
+        text: commercetoolsReview.text,
+        title: commercetoolsReview.title,
+        customerName:
+          commercetoolsReview.authorName ||
+          `${commercetoolsReview.customer?.obj?.firstName || ''} ${commercetoolsReview.customer?.obj?.lastName || ''}`,
+      }));
+    } catch (error) {
+      throw new Error(`Get subscriptions for account failed: ${error}`);
+    }
+  };
+}
