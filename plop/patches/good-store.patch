+import React, { useEffect, useState } from 'react';
+import { useRouter } from 'next/router';
+import { Menu } from '@headlessui/react';
+import { ChevronDownIcon as ArrowIcon } from '@heroicons/react/24/solid';
+import Typography from 'components/commercetools-ui/atoms/typography';
+import CloseIcon from 'components/icons/close';
+import { mapFacetIdentifierToLabel } from 'helpers/hooks/useFacet';
+import useI18n from 'helpers/hooks/useI18n';
+import { updateURLParams } from 'helpers/utils/updateURLParams';
+import { sdk } from 'sdk';
+import { RangeFacet } from 'types/facet';
+
+type Props = {
+  range?: RangeFacet;
+};
+
+const RangeFacet: React.FC<Props> = ({ range }) => {
+  const router = useRouter();
+  const [minPrice, setMinPrice] = useState<number>();
+  const [maxPrice, setMaxPrice] = useState<number>();
+  const [minSelectedPrice, setSelectedMinPrice] = useState<number>();
+  const [maxSelectedPrice, setSelectedMaxPrice] = useState<number>();
+  const [isFilled, setIsFilled] = useState(false);
+
+  const convertCents = (amountInCents: number) => Math.trunc(amountInCents / 100);
+  const { currencySymbol } = useI18n();
+
+  useEffect(() => {
+    console.log(sdk.currency);
+
+    if (range) {
+      const { min, max, minSelected, maxSelected } = range as RangeFacet;
+      const minConverted = convertCents(min!);
+      const maxConverted = convertCents(max!);
+
+      setMinPrice(minConverted);
+      setMaxPrice(maxConverted);
+
+      // Setting default values
+      if (typeof minSelected !== 'undefined' && typeof maxSelected !== 'undefined') {
+        setSelectedMinPrice(convertCents(minSelected));
+        setSelectedMaxPrice(convertCents(maxSelected));
+      } else {
+        setSelectedMinPrice(minConverted);
+        setSelectedMaxPrice(maxConverted);
+      }
+      console.log(min, max, minSelected, maxSelected);
+
+      setIsFilled(
+        convertCents(min ?? 0) !== convertCents(minSelected ?? 0) ||
+          convertCents(max ?? 0) !== convertCents(maxSelected ?? 0),
+      );
+    }
+  }, [router?.query]);
+
+  const handleSubmit = () => {
+    const params = [
+      { key: `facets[${range?.identifier}][min]`, value: `${minSelectedPrice ?? 0 * 100}` },
+      { key: `facets[${range?.identifier}][max]`, value: `${maxSelectedPrice ?? 0 * 100}` },
+    ];
+    const deleteParams = [];
+    if (!minSelectedPrice) {
+      deleteParams.push(`facets[${range?.identifier}][min]`);
+    }
+    if (!maxSelectedPrice) {
+      deleteParams.push(`facets[${range?.identifier}][max]`);
+    }
+    const currentURL = updateURLParams(params);
+
+    router.push(currentURL);
+  };
+
+  if (!range) {
+    return null;
+  }
+  return (
+    <Menu as="div" className="relative">
+      {({ open }) => (
+        <>
+          <Menu.Button>
+            <div
+              className={`mr-8 flex items-center rounded-md border border-gray-700
+              bg-transparent py-8 px-16 text-12 leading-[16px]
+               transition lg:text-16 ${isFilled ? 'bg-gray-700 text-white' : 'bg-transparent text-primary-black'}`}
+            >
+              <span className="mr-4 text-14">{mapFacetIdentifierToLabel[range.identifier]}</span>
+
+              <ArrowIcon className="mt-2 w-16" />
+            </div>
+          </Menu.Button>
+          <Menu.Items
+            static
+            className={`absolute left-0 flex min-w-[280px] origin-top-left translate-y-[10px] flex-row flex-wrap rounded-md bg-white p-20 shadow-lg transition ${
+              open ? 'opacity-1 z-20 scale-100' : 'z-[-1] scale-95 opacity-0'
+            }`}
+          >
+            <div className="flex flex-row items-center">
+              <div className="relative mr-10 flex w-80 flex-row items-center">
+                <input
+                  className="input-primary"
+                  type="number"
+                  placeholder="Min"
+                  min={minPrice}
+                  max={maxSelectedPrice}
+                  value={minSelectedPrice}
+                  onChange={(e) => setSelectedMinPrice(+e.target.value)}
+                ></input>
+                {minSelectedPrice !== minPrice && (
+                  <button
+                    className="absolute right-20 top-1/2 -translate-y-1/2"
+                    type="button"
+                    onClick={() => setSelectedMinPrice(minPrice)}
+                  >
+                    <CloseIcon className="h-10 w-10 cursor-pointer" />
+                  </button>
+                )}
+                <Typography className="ml-4 text-sm font-semibold">{currencySymbol}</Typography>
+              </div>
+              <Typography className="mr-10">––</Typography>
+              <div className="relative mr-20 flex w-80 flex-row items-center">
+                <input
+                  className="input-primary"
+                  type="number"
+                  placeholder="Max"
+                  min={minSelectedPrice}
+                  max={maxPrice}
+                  value={maxSelectedPrice}
+                  onChange={(e) => setSelectedMaxPrice(+e.target.value)}
+                ></input>
+                {maxSelectedPrice !== maxPrice && (
+                  <button
+                    className="absolute right-16 top-1/2 -translate-y-1/2"
+                    type="button"
+                    onClick={() => setSelectedMaxPrice(maxPrice)}
+                  >
+                    <CloseIcon className=" h-10 w-10 cursor-pointer" />
+                  </button>
+                )}
+                <Typography className="ml-4 text-sm font-semibold">{currencySymbol}</Typography>
+              </div>
+              <button className="h-40 w-60 bg-gray-700 text-white" type="button" onClick={handleSubmit}>
+                Go
+              </button>
+            </div>
+          </Menu.Items>
+        </>
+      )}
+    </Menu>
+  );
+};
+
+export default RangeFacet;
