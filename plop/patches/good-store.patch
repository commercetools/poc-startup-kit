+import { FC } from 'react';
+import router from 'next/router';
+import { Variant, Variant as VariantType } from '@commercetools/frontend-domain-types/product/Variant';
+import Typography from 'components/commercetools-ui/atoms/typography';
+import { CurrencyHelpers } from 'helpers/currencyHelpers';
+
+type VariantProps = {
+  variants: VariantType[];
+  onClick?: (sku: string) => void;
+  attribute: string;
+  currentVariantId?: string;
+  isVariantGroup?: boolean;
+};
+
+const discountPercentage = (variant: Variant) => {
+  return (
+    variant.discountedPrice &&
+    (((variant.price?.centAmount as number) - (variant.discountedPrice.centAmount as number)) /
+      (variant.price?.centAmount as number)) *
+      100
+  );
+};
+
+const getPriceValue = (variant: Variant) => {
+  const discount = discountPercentage(variant);
+  if (discount) {
+    return discount;
+  }
+  return variant.price?.centAmount;
+};
+
+const groupedVariants = (items: Variant[], variantSelector: string) => {
+  const variants = [...items];
+  const grouped: Variant[][] = [];
+  while (variants.length) {
+    const [current] = variants.splice(0, 1);
+    const currentAttributeValue = getPriceValue(current);
+    const index = grouped.findIndex(
+      (group) => getPriceValue(group?.[0]) === currentAttributeValue && typeof currentAttributeValue !== 'undefined',
+    );
+    if (index > -1) {
+      grouped[index].push(current);
+    } else if (typeof currentAttributeValue !== 'undefined') {
+      grouped.push([current]);
+    }
+  }
+  return grouped;
+};
+
+const ColorVariant: FC<VariantProps> = ({ variants, onClick, attribute, currentVariantId, isVariantGroup }) => {
+  const grouped = isVariantGroup ? groupedVariants(variants, attribute) : [];
+
+  return (
+    <>
+      {!isVariantGroup && (
+        <div className="mt-15 flex gap-24">
+          {variants?.map(({ attributes, id, sku }, index) => (
+            <div
+              key={index}
+              className={`h-20 w-20 rounded-full ${
+                id == currentVariantId ? 'border-2 border-neutral-500' : 'border border-neutral-300'
+              } ${variants.length > 1 ? 'hover:cursor-pointer' : 'pointer-events-none'} `}
+              style={{ backgroundColor: attributes?.[attribute].key }}
+              onClick={() => onClick?.(sku)}
+            />
+          ))}
+        </div>
+      )}
+      {isVariantGroup && (
+        <div className="flex flex-col gap-24">
+          {grouped?.map((group, i) => (
+            <div key={i}>
+              {discountPercentage(group[0]) ? (
+                <div className="flex flex-row justify-between">
+                  <div className="flex items-center gap-8">
+                    <Typography fontSize={16} className="block font-regular leading-loose text-accent-red 2xl:text-18">
+                      {CurrencyHelpers.formatForCurrency(group[0].discountedPrice as number, router.locale)}
+                    </Typography>
+                    <Typography className="block leading-loose text-gray-500 line-through 2xl:text-16">
+                      {CurrencyHelpers.formatForCurrency(group[0].price ?? 0, router.locale)}
+                    </Typography>
+                  </div>
+
+                  {
+                    <span className="ml-8 mt-10 mb-8 flex h-[25px] w-[45px] items-center justify-center bg-accent-red text-11 text-neutral-100">
+                      {Math.round(discountPercentage(group[0]) ?? 0)}%
+                    </span>
+                  }
+                </div>
+              ) : (
+                <Typography className="block leading-loose 2xl:text-20">
+                  {CurrencyHelpers.formatForCurrency(group[0].price as number, router.locale)}
+                </Typography>
+              )}
+              <div className="mt-15 flex gap-24">
+                {group?.map(({ attributes, id, sku }, index) => (
+                  <div
+                    key={index}
+                    className={`h-20 w-20 rounded-full ${
+                      id == currentVariantId ? 'border-2 border-neutral-500' : 'border border-neutral-300'
+                    } ${variants.length > 1 ? 'hover:cursor-pointer' : 'pointer-events-none'} `}
+                    style={{ backgroundColor: attributes?.[attribute].key }}
+                    onClick={() => onClick?.(sku)}
+                  />
+                ))}
+              </div>
+            </div>
+          ))}
+        </div>
+      )}
+    </>
+  );
+};
+
+export default ColorVariant;
