+import React, { useCallback, useRef } from 'react';
+import useResizeObserver from 'helpers/hooks/useResizeObserver';
+import AnnouncementBar, { Props as AnnouncementBarProps } from 'components/commercetools-ui/organisms/bar/announcement';
+import { NextFrontasticImage } from 'frontastic/lib/image';
+import { Reference } from 'types/reference';
+import HeaderNavigationMobile from 'components/commercetools-ui/organisms/header/header-navigation/header-navigation-mobile';
+import HeaderLogo from 'components/commercetools-ui/organisms/header/header-logo';
+import Link from 'components/commercetools-ui/atoms/link';
+import Typography from 'components/commercetools-ui/atoms/typography';
+
+export interface CheckoutHeaderProps {
+  logo: NextFrontasticImage;
+  logoMobile: NextFrontasticImage;
+  logoLink: Reference;
+  logoLinkMobile: Reference;
+  shopLink: Reference;
+  shopText: string;
+}
+
+interface Props {
+  data: CheckoutHeaderProps & AnnouncementBarProps;
+}
+
+const CheckoutHeaderTastic = ({ data }: Props) => {
+  const headerRef = useRef<HTMLDivElement>(null);
+
+  const setPaddingTop = useCallback(() => {
+    if (headerRef.current) {
+      document.body.style.paddingTop = `${headerRef.current.clientHeight - 1}px`;
+    }
+  }, []);
+
+  useResizeObserver(headerRef, setPaddingTop);
+
+  const announcementBarData = {
+    text: data.text,
+    highlightedSubstring: data.highlightedSubstring,
+    target: data.target,
+  };
+
+  return (
+    <div className="fixed top-0 z-50 w-full" ref={headerRef}>
+      {announcementBarData && <AnnouncementBar {...announcementBarData} />}
+      <header className="relative flex h-fit w-full border-b-[1.5px] border-neutral-400 bg-white">
+        <Link className="absolute top-1/2 ml-48 hidden -translate-y-1/2 lg:block" link={data.logoLink}>
+          <Typography>{`← ${data.shopText ?? 'Continue shopping'}`}</Typography>
+        </Link>
+        <div aria-label="Top" className="flex w-full items-center justify-center px-16 md:px-28 lg:px-48">
+          <HeaderNavigationMobile logo={data.logoMobile} logoLink={data.logoLinkMobile} links={[]} />
+          <div className="flex w-full justify-center self-center lg:w-fit">
+            <HeaderLogo
+              logo={data.logo}
+              logoLink={data.logoLink}
+              imageClassName="flex h-44 w-200 justify-center text-center text-16 font-bold md:h-76 md:w-214 md:text-28"
+            />
+          </div>
+        </div>
+      </header>
+    </div>
+  );
+};
+export default CheckoutHeaderTastic;
