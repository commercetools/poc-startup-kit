+import React, { HTMLAttributes } from 'react';
+import { Variant as VariantType } from '@commercetools/frontend-domain-types/product/Variant';
+import Variant from 'components/commercetools-ui/organisms/variant';
+import GridVariantSelector from './grid-variant-selector';
+import { UIProduct } from './types';
+
+type Props = {
+  product: UIProduct;
+  variant: VariantType;
+  onChangeVariant: (sku: string) => void;
+  inModalVersion?: boolean;
+  isVariantGroup?: boolean;
+};
+
+export const attributesToDisplay = ['color', 'size'];
+
+export const getAttributeValue = (attributeKey: string, attributes?: Record<string, any>): any => {
+  if (typeof attributes?.[attributeKey] === 'object') {
+    return attributes?.[attributeKey]?.key;
+  }
+  return attributes?.[attributeKey];
+};
+
+export const getAttributeLabel = (attributeKey: string, attributes?: Record<string, any>): any => {
+  if (typeof attributes?.[attributeKey] === 'object') {
+    return attributes?.[attributeKey]?.label;
+  }
+  return attributes?.[attributeKey];
+};
+
+export const uniqueAttributeValues = (variants: VariantType[], attributeKey: string): string[] => {
+  return variants
+    ?.filter((item) => typeof item.attributes?.[attributeKey] !== 'undefined')
+    ?.map((item) => getAttributeValue(attributeKey, item.attributes))
+    .filter((item, i, self) => self.indexOf(item) === i);
+};
+
+const VariantSelector: React.FC<Props & HTMLAttributes<HTMLDivElement>> = ({
+  product,
+  variant,
+  className,
+  inModalVersion,
+  isVariantGroup,
+  onChangeVariant,
+}) => {
+  const isFirstAttributeAvailable =
+    attributesToDisplay?.length > 1
+      ? uniqueAttributeValues(product.variants, attributesToDisplay[0]).length > 0
+      : false;
+
+  const isSecondAttributeAvailable =
+    attributesToDisplay?.length > 1
+      ? uniqueAttributeValues(product.variants, attributesToDisplay[1]).length > 1
+      : false;
+
+  if (isFirstAttributeAvailable && (!isSecondAttributeAvailable || inModalVersion)) {
+    return (
+      <Variant
+        className="mt-25 border-b border-b-neutral-400 pb-20"
+        variants={product?.variants}
+        currentVariant={variant}
+        attribute={attributesToDisplay[0]}
+        inModalVersion={inModalVersion}
+        isVariantGroup={isVariantGroup}
+        onClick={onChangeVariant}
+      />
+    );
+  }
+  if (attributesToDisplay?.length > 1 && isSecondAttributeAvailable) {
+    return (
+      <GridVariantSelector
