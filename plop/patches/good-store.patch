+          {resendPasswordReset && (
+            <Link variant="menu-item" className="mx-auto block w-fit text-14" link={signInLink} onClick={backToLogin}>
+              {formatAccountMessage({ id: 'account.back.sign', defaultMessage: 'Back to sign in' })}
+            </Link>
+          )}
+        </form>
+      </div>
+      <AlterForm page="register" />
+    </>
+  );
+};
