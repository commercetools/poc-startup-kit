+import { updateURLParams } from 'helpers/utils/updateURLParams';
+import { useRouter } from 'next/router';
+import { useEffect, useState } from 'react';
+
+export const mapFacetIdentifierToLabel: Record<string, string> = {
+  'variants.attributes.commonSize': 'Size',
+  'variants.price': 'Price',
+};
+function useFacet<S = undefined>(attribute: string): { currentParam: S | undefined; handleChange: (value: S) => void } {
+  const router = useRouter();
+  const [currentParam, setCurrentParam] = useState<S>();
+
+  const handleChange = (value: S) => {
+    const params: { key: string; value: string }[] = [];
+    const deleteParams: string[] = [];
+    setCurrentParam(value);
+
+    if (value === currentParam) {
+      deleteParams.push(attribute);
+    } else {
+      params.push({
+        key: attribute,
+        value: value as string,
+      });
+    }
+
+    const currentURL = updateURLParams(params, deleteParams);
+
+    router.push(currentURL);
+  };
+  useEffect(() => {
+    const defaultParam = router?.query[attribute] as S;
+    setCurrentParam(defaultParam);
+  }, [router?.query]);
+
+  return {
+    currentParam,
+    handleChange,
+  };
+}
+
+export default useFacet;
